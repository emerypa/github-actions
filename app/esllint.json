{
  "extends": ["airbnb", "prettier", "prettier/standard", "prettier/react", "plugin:react/recommended"],
  "plugins": ["prettier", "react", "jest"],
  "rules": {
    "prettier/prettier": "error",
    "no-param-reassign": 0,
    "array-callback-return": 0,
    "no-nested-ternary": 0,
    "react/no-array-index-key": 0,
    "react/state-in-constructor": 0
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}
